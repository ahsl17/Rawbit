    x:DataType="root:MainWindowViewModel"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:root="clr-namespace:Rawbit.UI.Root">

    <DockPanel>
        <ContentControl Content="{Binding Content}" />
    </DockPanel>
</Window>
